{
  "log_level": "debug",
  "rules": [
    {
      "name": "test",
      "listen": ":1234",
      "enable_regexp": false,
      "first_packet_timeout": 5000,
      "targets": [
        {
          "regexp": "^(GET|POST|HEAD|DELETE|PUT|CONNECT|OPTIONS|TRACE)",
          "address": "127.0.0.1:80"
        },
        {
          "regexp": "^SSH",
          "address": "127.0.0.1:22"
        }
      ]
    }
  ]
}